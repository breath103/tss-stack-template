{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "../../scripts/with-env.sh vite",
    "deploy": "npm run script -- scripts/deploy.ts",
    "destroy": "npm run script -- scripts/destroy.ts",
    "script": "tsx --tsconfig scripts/tsconfig.json",
    "build:types": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "lint": "eslint src scripts",
    "test": "../../scripts/with-env.sh --env=test vitest run",
    "test:watch": "../../scripts/with-env.sh --env=test vitest"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-router": "^1.157.16",
    "better-auth": "^1.4.18",
    "dotenv": "^16.4.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.978.0",
    "@aws-sdk/client-ssm": "^3.978.0",
    "@eslint/js": "^9.39.2",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-better-tailwindcss": "^4.0.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "zod": "^4.3.5"
  }
